      "example": "OBSERVE(\"fluorescence\", expected=\"GFP expression\", record=true)"
    },
    {
      "id": 25,
      "name": "CHECK_POINT",
      "category": "OBSERVATION_OPERATIONS",
      "parameters": [
        {"name": "condition", "type": "string expression", "required": true},
        {"name": "pass_action", "type": "string", "required": true, "options": ["continue", "proceed_to_step"]},
        {"name": "fail_action", "type": "string", "required": true, "options": ["repeat", "stop", "adjust"]}
