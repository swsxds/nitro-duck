    },
    {
      "id": 37,
      "name": "INFILTRATE_PLANTS",
      "category": "PLANT_INFILTRATION_OPERATIONS",
      "parameters": [
        {"name": "infiltration_method", "type": "string", "required": true, "options": ["vacuum", "syringe", "agitation"]},
