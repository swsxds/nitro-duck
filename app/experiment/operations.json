{
  "atomic_operations": [
    {
      "id": 1,
      "name": "ADD_SUBSTANCE",
      "category": "SUBSTANCE_OPERATIONS",
      "parameters": [
        {"name": "substance_name", "type": "string", "required": true},
        {"name": "volume", "type": "number + unit", "required": true, "units": ["µL", "mL", "L"]},
        {"name": "concentration", "type": "number + unit", "required": false},
        {"name": "container", "type": "string", "required": true, "options": ["tube", "flask", "plate", "etc"]},
        {"name": "mixing_method", "type": "string", "required": false, "options": ["vortex", "invert", "pipette"]}
      ],
      "example": "ADD_SUBSTANCE(\"CTAB buffer\", 5, \"mL\", container=\"15mL tube\")"
    },
    {
      "id": 2,
      "name": "ADD_SOLID",
      "category": "SUBSTANCE_OPERATIONS",
      "parameters": [
        {"name": "substance_name", "type": "string", "required": true},
        {"name": "amount", "type": "number + unit", "required": true, "units": ["mg", "g", "kg"]},
        {"name": "container", "type": "string", "required": true}
      ],
      "example": "ADD_SOLID(\"agar\", 15, \"g\", container=\"flask\")"
    },
    {
      "id": 3,
      "name": "REMOVE_SUBSTANCE",
      "category": "SUBSTANCE_OPERATIONS",
      "parameters": [
        {"name": "removal_method", "type": "string", "required": true, "options": ["pipette", "decant", "aspirate"]},
        {"name": "volume", "type": "mixed", "required": false, "options": ["number + unit", "supernatant", "all", "pellet"]},
        {"name": "keep_remainder", "type": "boolean", "required": false}
      ],
      "example": "REMOVE_SUBSTANCE(\"decant\", \"supernatant\", keep_pellet=true)"
    },
    {
      "id": 4,
      "name": "SET_TEMPERATURE",
      "category": "TEMPERATURE_OPERATIONS",
      "parameters": [
        {"name": "temperature", "type": "number + unit", "required": true, "units": ["°C"]},
        {"name": "duration", "type": "number + unit", "required": true, "units": ["min", "h", "overnight"]},
        {"name": "device", "type": "string", "required": true, "options": ["incubator", "water_bath", "heat_block", "ice", "freezer", "fridge"]},
        {"name": "shaking", "type": "number + rpm", "required": false}
      ],
      "example": "SET_TEMPERATURE(55, \"°C\", 60, \"min\", device=\"incubator\")"
    },
    {
      "id": 5,
      "name": "COOL",
      "category": "TEMPERATURE_OPERATIONS",
      "parameters": [
        {"name": "target_temp", "type": "mixed", "required": true, "options": ["number + unit", "RT", "ice", "4°C", "-20°C", "-80°C"]},
        {"name": "duration", "type": "number + unit", "required": true},
        {"name": "method", "type": "string", "required": true, "options": ["bench", "ice", "fridge", "freezer"]}
      ],
      "example": "COOL(\"ice\", 15, \"min\")"
    },
    {
      "id": 6,
      "name": "MIX",
      "category": "MIXING_OPERATIONS",
      "parameters": [
        {"name": "method", "type": "string", "required": true, "options": ["vortex", "invert", "shake", "stir", "pipette"]},
        {"name": "duration", "type": "number + unit", "required": true},
        {"name": "intensity", "type": "string", "required": false, "options": ["gentle", "moderate", "vigorous", "or rpm"]}
      ],
      "example": "MIX(\"vortex\", 30, \"sec\", intensity=\"vigorous\")"
    },
    {
      "id": 7,
      "name": "RESUSPEND",
      "category": "MIXING_OPERATIONS",
      "parameters": [
        {"name": "substance_added", "type": "string", "required": true},
        {"name": "volume", "type": "number + unit", "required": true},
        {"name": "method", "type": "string", "required": true, "options": ["pipette", "vortex", "gentle_swirl"]}
      ],
      "example": "RESUSPEND(\"MQ water\", 300, \"µL\", method=\"pipette\")"
    },
    {
      "id": 8,
      "name": "CENTRIFUGE",
      "category": "SEPARATION_OPERATIONS",
      "parameters": [
        {"name": "speed", "type": "number + unit", "required": true, "units": ["g", "rpm"]},
        {"name": "duration", "type": "number + unit", "required": true},
        {"name": "temperature", "type": "mixed", "required": false, "options": ["number + °C", "cooled", "RT"]}
      ],
      "example": "CENTRIFUGE(13000, \"g\", 15, \"min\", temperature=4)"
    },
    {
      "id": 9,
      "name": "FILTER",
      "category": "SEPARATION_OPERATIONS",
      "parameters": [
        {"name": "filter_size", "type": "number + unit", "required": true, "units": ["µm", "kDa"]},
        {"name": "method", "type": "string", "required": true, "options": ["syringe", "vacuum", "gravity"]}
      ],
      "example": "FILTER(0.22, \"µm\", method=\"syringe\")"
    },
    {
      "id": 10,
      "name": "GRIND",
      "category": "PHYSICAL_MANIPULATION",
      "parameters": [
        {"name": "sample_type", "type": "string", "required": true},
        {"name": "method", "type": "string", "required": true, "options": ["mortar_pestle", "homogenizer", "bead_beater"]},
        {"name": "state", "type": "string", "required": true, "options": ["frozen", "fresh", "dried"]},
        {"name": "target_consistency", "type": "string", "required": true, "options": ["powder", "paste", "suspension"]}
      ],
      "example": "GRIND(\"leaf tissue\", method=\"mortar_pestle\", state=\"frozen\", target=\"powder\")"
    },
    {
      "id": 11,
      "name": "TRANSFER",
      "category": "PHYSICAL_MANIPULATION",
      "parameters": [
        {"name": "source_container", "type": "string", "required": true},
        {"name": "destination_container", "type": "string", "required": true},
        {"name": "volume", "type": "mixed", "required": true, "options": ["number + unit", "all", "supernatant", "upper_phase"]},
        {"name": "method", "type": "string", "required": true, "options": ["pipette", "pour", "decant"]}
      ],
      "example": "TRANSFER(\"tube1\", \"tube2\", \"upper_phase\", method=\"pipette\")"
    },
    {
      "id": 12,
      "name": "MEASURE",
      "category": "MEASUREMENT_OPERATIONS",
      "parameters": [
        {"name": "measurement_type", "type": "string", "required": true, "options": ["OD", "pH", "concentration", "weight", "volume"]},
        {"name": "device", "type": "string", "required": true, "options": ["spectrophotometer", "pH_meter", "nanodrop", "scale"]},
        {"name": "wavelength", "type": "number", "required": false},
        {"name": "record_as", "type": "string", "required": true}
      ],
      "example": "MEASURE(\"OD\", device=\"spectrophotometer\", wavelength=600, record_as=\"culture_OD\")"
    },
    {
      "id": 13,
      "name": "ADJUST",
      "category": "MEASUREMENT_OPERATIONS",
      "parameters": [
        {"name": "parameter", "type": "string", "required": true, "options": ["pH", "concentration", "volume", "OD"]},
        {"name": "target_value", "type": "number", "required": true},
        {"name": "adjustment_substance", "type": "string", "required": true},
        {"name": "method", "type": "string", "required": true, "options": ["dropwise", "calculated_addition"]}
      ],
      "example": "ADJUST(\"pH\", 5.5, adjustment_substance=\"NaOH\", method=\"dropwise\")"
    },
    {
      "id": 14,
      "name": "INCUBATE",
      "category": "TIME_OPERATIONS",
      "parameters": [
        {"name": "duration", "type": "mixed", "required": true, "options": ["number + unit", "overnight"]},
        {"name": "conditions", "type": "string", "required": true, "options": ["static", "shaking", "dark", "light"]},
        {"name": "temperature", "type": "number + °C", "required": true},
        {"name": "rpm", "type": "number", "required": false}
      ],
      "example": "INCUBATE(\"overnight\", conditions=\"shaking\", temperature=28, rpm=200)"
    },
    {
      "id": 15,
      "name": "WAIT",
      "category": "TIME_OPERATIONS",
      "parameters": [
        {"name": "duration", "type": "number + unit", "required": true},
        {"name": "purpose", "type": "string", "required": false}
      ],
      "example": "WAIT(5, \"min\", purpose=\"let bubbles settle\")"
    },
    {
      "id": 16,
      "name": "WASH",
      "category": "WASHING_OPERATIONS",
      "parameters": [
        {"name": "wash_solution", "type": "string", "required": true},
        {"name": "volume", "type": "number + unit", "required": true},
        {"name": "repetitions", "type": "number", "required": true},
        {"name": "removal_method", "type": "string", "required": true, "options": ["aspirate", "decant", "pipette"]}
      ],
      "example": "WASH(\"70% ethanol\", 500, \"µL\", repetitions=2, removal_method=\"aspirate\")"
    },
    {
      "id": 17,
      "name": "DRY",
      "category": "DRYING_OPERATIONS",
      "parameters": [
        {"name": "method", "type": "string", "required": true, "options": ["air_dry", "vacuum_dry", "speed_vac"]},
        {"name": "duration", "type": "number + unit", "required": true},
        {"name": "temperature", "type": "number + °C", "required": false}
      ],
      "example": "DRY(\"air_dry\", 15, \"min\", temperature=37)"
    },
    {
      "id": 18,
      "name": "USE_EQUIPMENT",
      "category": "SPECIALIZED_EQUIPMENT_OPERATIONS",
      "parameters": [
        {"name": "equipment", "type": "string", "required": true, "options": ["thermocycler", "vacuum_pump", "sonicator", "electroporator"]},
        {"name": "program/settings", "type": "string or dict", "required": true},
        {"name": "duration", "type": "number + unit", "required": false}
      ],
      "example": "USE_EQUIPMENT(\"thermocycler\", program=\"golden_gate\", cycles=25)"
    },
    {
      "id": 19,
      "name": "APPLY_VACUUM",
      "category": "SPECIALIZED_EQUIPMENT_OPERATIONS",
      "parameters": [
        {"name": "pressure", "type": "number + unit", "required": true, "units": ["kPa", "bar"]},
        {"name": "duration", "type": "number + unit", "required": true},
        {"name": "release_speed", "type": "string", "required": true, "options": ["slow", "fast"]}
      ],
      "example": "APPLY_VACUUM(-70, \"kPa\", 10, \"min\", release_speed=\"slow\")"
    },
    {
      "id": 20,
      "name": "CALCULATE",
      "category": "CALCULATION_OPERATIONS",
      "parameters": [
        {"name": "formula", "type": "string expression", "required": true},
        {"name": "input_variables", "type": "dict", "required": true},
        {"name": "output_variable", "type": "string", "required": true},
        {"name": "unit", "type": "string", "required": true}
      ],
      "example": "CALCULATE(\"(c1 * v1) / c2\", {c1: \"culture_OD\", v1: 100, c2: 0.7}, output=\"volume_needed\", unit=\"mL\")"
    },
    {
      "id": 21,
      "name": "INOCULATE",
      "category": "BIOLOGICAL_OPERATIONS",
      "parameters": [
        {"name": "source", "type": "string", "required": true, "options": ["colony", "glycerol_stock", "culture"]},
        {"name": "destination_medium", "type": "string", "required": true},
        {"name": "volume", "type": "mixed", "required": true, "options": ["number + unit", "single_colony", "loop", "tip"]},
        {"name": "dilution_ratio", "type": "string", "required": false}
      ],
      "example": "INOCULATE(\"glycerol_stock\", \"LB+antibiotics\", method=\"tip\")"
    },
    {
      "id": 22,
      "name": "PLATE",
      "category": "BIOLOGICAL_OPERATIONS",
      "parameters": [
        {"name": "volume", "type": "number + unit", "required": true},
        {"name": "plate_type", "type": "string", "required": true},
        {"name": "spreading_method", "type": "string", "required": true, "options": ["spread", "streak", "pour"]}
      ],
      "example": "PLATE(100, \"µL\", plate_type=\"LB+kan\", method=\"spread\")"
    },
    {
      "id": 23,
      "name": "TRANSFORM",
      "category": "BIOLOGICAL_OPERATIONS",
      "parameters": [
        {"name": "cells_type", "type": "string", "required": true},
        {"name": "dna_volume", "type": "number + unit", "required": true},
        {"name": "method", "type": "string", "required": true, "options": ["heat_shock", "electroporation", "chemical"]},
        {"name": "recovery_time", "type": "number + unit", "required": true}
      ],
      "example": "TRANSFORM(\"competent_cells\", 5, \"µL\", method=\"heat_shock\", recovery=1, \"h\")"
    },
    {
      "id": 24,
      "name": "OBSERVE",
      "category": "OBSERVATION_OPERATIONS",
      "parameters": [
        {"name": "observation_type", "type": "string", "required": true, "options": ["color_change", "precipitation", "growth", "fluorescence"]},
        {"name": "expected_result", "type": "string", "required": false},
        {"name": "record", "type": "boolean", "required": true}
