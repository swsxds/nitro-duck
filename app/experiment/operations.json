    },
    {
      "id": 38,
      "name": "PREPARE_AGROBACTERIUM",
      "category": "TRANSFORMATION_SPECIFIC_OPERATIONS",
      "parameters": [
        {"name": "strain", "type": "string", "required": true},
        {"name": "pre_culture_volume", "type": "number + unit", "required": true},
        {"name": "target_OD", "type": "number", "required": true},
        {"name": "dilution_strategy", "type": "string", "required": true, "options": ["ratio", "calculated"]},
