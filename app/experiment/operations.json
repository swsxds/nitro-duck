      "example": "CHECK_POINT(\"OD >= 0.6\", pass=\"continue\", fail=\"wait_and_remeasure\")"
    },
    {
      "id": 26,
      "name": "SAFETY_STEP",
      "category": "SAFETY_OPERATIONS",
      "parameters": [
        {"name": "equipment", "type": "string", "required": true, "options": ["gloves", "goggles", "fume_hood", "biosafety_cabinet"]},
        {"name": "action", "type": "string", "required": true, "options": ["wear", "use", "work_in"]}
      ],
      "example": "SAFETY_STEP(\"fume_hood\", \"work_in\")"
    },
    {
      "id": 27,
      "name": "MANUAL_STEP",
      "category": "UNIVERSAL_MANUAL_OPERATION",
      "parameters": [
        {"name": "description", "type": "free text string", "required": true},
        {"name": "duration", "type": "estimated time", "required": false},
        {"name": "critical", "type": "boolean", "required": true},
        {"name": "image", "type": "reference image path", "required": false},
        {"name": "video", "type": "reference video path", "required": false},
        {"name": "notes", "type": "additional tips/warnings", "required": false}
