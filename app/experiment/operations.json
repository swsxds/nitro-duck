      "example": "CRITICAL_TRANSFER(source: \"tube1\", destination: \"tube2\", phase: \"upper\", avoidance_zone: \"interphase\", fallback_action: \"repeat_centrifugation\", precision_required: \"high\")"
    },
    {
      "id": 32,
      "name": "STORE_OVERNIGHT",
      "category": "EXTENDED_STORAGE_OPERATIONS",
      "parameters": [
        {"name": "temperature", "type": "number + °C", "required": true},
        {"name": "container", "type": "string", "required": true},
        {"name": "protective_measures", "type": "string", "required": false, "options": ["wrap_in_foil", "parafilm", "vacuum_seal"]},
        {"name": "stability_notes", "type": "string", "required": false}
      ],
      "example": "STORE_OVERNIGHT(temperature: -20, container: \"2mL tube\", protective_measures: \"none\", stability_notes: \"Stable for up to 1 month\")"
    },
    {
      "id": 33,
      "name": "HAZARD_WARNING",
      "category": "HAZARD_AWARENESS_OPERATIONS",
      "parameters": [
        {"name": "hazard_type", "type": "string", "required": true, "options": ["chemical", "biological", "physical"]},
        {"name": "severity", "type": "string", "required": true, "options": ["low", "medium", "high", "extreme"]},
        {"name": "protective_equipment", "type": "list", "required": true},
        {"name": "ventilation_required", "type": "boolean", "required": true}
      ],
      "example": "HAZARD_WARNING(hazard_type: \"chemical\", severity: \"high\", protective_equipment: [\"gloves\", \"goggles\", \"lab_coat\"], ventilation_required: true)"
    },
    {
      "id": 34,
      "name": "SUBCULTURE_PLANTS",
      "category": "PLANT_CULTURE_OPERATIONS",
      "parameters": [
        {"name": "plant_species", "type": "string", "required": true},
        {"name": "source_density", "type": "percentage", "required": true},
        {"name": "target_density", "type": "percentage", "required": true},
        {"name": "medium_type", "type": "string", "required": true},
        {"name": "container", "type": "string", "required": true, "options": ["tray", "pot", "plate"]},
        {"name": "growth_conditions", "type": "string", "required": true}
