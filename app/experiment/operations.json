        {"name": "target_phase", "type": "string", "required": true, "options": ["upper", "lower", "interphase", "aqueous", "organic"]},
        {"name": "caution_notes", "type": "string", "required": true},
        {"name": "validation_method", "type": "string", "required": true, "options": ["visual", "marker_dye"]}
      ],
      "example": "SEPARATE_PHASES(mixture_type: \"aqueous_organic\", expected_phases: 2, target_phase: \"upper\", caution_notes: \"Do not disturb interphase!\", validation_method: \"visual\")"
    },
    {
      "id": 29,
      "name": "VISUALIZE_PELLET",
      "category": "VISUALIZATION_OPERATIONS",
      "parameters": [
        {"name": "expected_appearance", "type": "string", "required": true},
        {"name": "method", "type": "string", "required": true, "options": ["swirl", "visual_inspection", "UV_light", "microscope"]},
