      "example": "VISUALIZE_PELLET(expected_appearance: \"white DNA fibers\", method: \"swirl\", timeout: \"15 min\", troubleshooting_notes: \"If no fibers appear, leave on ice longer\")"
    },
    {
      "id": 30,
      "name": "SCALE_VOLUMES",
      "category": "SCALING_OPERATIONS",
      "parameters": [
        {"name": "scale_factor", "type": "number", "required": true},
        {"name": "reference_step", "type": "string", "required": true},
        {"name": "adjust_subsequent", "type": "boolean", "required": true},
        {"name": "calculation_method", "type": "string", "required": true, "options": ["linear", "proportional", "custom"]}
      ],
      "example": "SCALE_VOLUMES(scale_factor: 2, reference_step: \"CTAB_volume\", adjust_subsequent: true, calculation_method: \"proportional\")"
    },
    {
      "id": 31,
      "name": "CRITICAL_TRANSFER",
      "category": "CRITICAL_TRANSFER_OPERATIONS",
      "parameters": [
        {"name": "source", "type": "string", "required": true},
        {"name": "destination", "type": "string", "required": true},
        {"name": "phase", "type": "string", "required": true, "options": ["upper", "lower", "aqueous", "organic", "supernatant", "pellet"]},
        {"name": "avoidance_zone", "type": "string", "required": true, "options": ["interphase", "pellet", "debris"]},
        {"name": "fallback_action", "type": "string", "required": true, "options": ["repeat", "discard", "adjust_technique"]},
        {"name": "precision_required", "type": "string", "required": true, "options": ["high", "medium", "low"]}
