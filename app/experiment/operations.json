    },
    {
      "id": 35,
      "name": "SELECT_PLANT_MATERIAL",
      "category": "PLANT_SELECTION_OPERATIONS",
      "parameters": [
        {"name": "criteria", "type": "string", "required": true, "options": ["confluence", "age", "health_status", "developmental_stage"]},
        {"name": "target_confluence", "type": "percentage", "required": true},
        {"name": "quality_notes", "type": "string", "required": true}
