    },
    {
      "id": 36,
      "name": "PRE_TREAT_PLANTS",
      "category": "PLANT_PRE_TREATMENT_OPERATIONS",
      "parameters": [
        {"name": "treatment_solution", "type": "string", "required": true},
        {"name": "concentration", "type": "number + unit", "required": true},
        {"name": "temperature", "type": "number + °C", "required": true},
        {"name": "duration", "type": "number + unit", "required": true},
        {"name": "method", "type": "string", "required": true, "options": ["submersion", "spraying", "vacuum_infiltration"]}
